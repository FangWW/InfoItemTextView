<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#FFFFFF"/>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"
                     android:topLeftRadius="0.1px" android:topRightRadius="0.1px"/>
            <stroke android:width="1px" android:color="#ffa8abad"/>
        </shape>
    </item>
    <item android:bottom="1px" android:left="1px" android:right="1px" android:top="1px">
        <shape>
            <solid android:color="#FFFFFF"/>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"
                     android:topLeftRadius="0.1px" android:topRightRadius="0.1px"/>
            <stroke android:width="1px" android:color="#ffffffff"/>
        </shape>
    </item>
</layer-list>